{
  "name": "movie_library",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:build": "npm run build && npm run preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:css": "stylelint **/*.css --fix",
    "lint:all": "npm run lint && npm run lint:css",
    "prettier": "prettier --check --ignore-unknown src/",
    "prettier:fix": "prettier ./src --write --ignore-unknown"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@typescript-eslint/typescript-estree": "^8.29.1",
    "@typescript-eslint/utils": "^8.29.1",
    "appwrite": "^17.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.5.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-dom": "^1.41.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-react-x": "^1.41.0",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-tailwindcss": "^0.0.7",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
